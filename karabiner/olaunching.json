// I no longer use o launching as RCMD has replaced that need for me, but if you ever want to use it here if the config snippet that I used.
{
    "description": "o Launcher",
    "manipulators": [
        {
            "from": {
                "modifiers": {
                    "mandatory": []
                },
                "simultaneous": [
                    {
                        "key_code": "open_bracket"
                    },
                    {
                        "key_code": "f"
                    }
                ],
                "simultaneous_options": {
                    "key_down_order": "strict",
                    "key_up_order": "strict_inverse",
                    "to_after_key_up": [
                        {
                            "set_variable": {
                                "name": "launcher_mode_v4",
                                "value": 0
                            }
                        }
                    ]
                }
            },
            "to": [
                {
                    "set_variable": {
                        "name": "launcher_mode_v4",
                        "value": 1
                    }
                },
                {
                    "shell_command": "open https://www.figma.com/files/recent"
                }
            ],
            "type": "basic"
